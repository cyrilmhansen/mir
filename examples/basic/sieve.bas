5 REM Sieve of Eratosthenes benchmark
10 S = 8190
20 N = 100
30 DIM A(8190)
40 FOR J = 1 TO N
50  FOR I = 0 TO S - 1
60   A(I) = 1
70  NEXT I
80  COUNT = 0
90  FOR I = 2 TO S - 1
100  IF A(I)=0 THEN 170
110  PRIME = I + 1
120  FOR K = I + PRIME TO S - 1 STEP PRIME
130   A(K) = 0
140  NEXT K
150  COUNT = COUNT + 1
160  REM
170 NEXT I
180 NEXT J
190 PRINT COUNT
200 END
