10 D=14
20 N=1
30 FOR I=1 TO D:N=N*10:NEXT I
40 T0=TIMER
50 A1=1/5
60 A1SQ=A1*A1
70 S1=0
80 T=A1
90 I=1
100 SIGN=1
110 WHILE ABS(T)>1/N
120 S1=S1+SIGN*T/I
130 T=T*A1SQ
140 I=I+2
150 SIGN=-SIGN
160 WEND
170 A2=1/239
180 A2SQ=A2*A2
190 S2=0
200 T=A2
210 I=1
220 SIGN=1
230 WHILE ABS(T)>1/N
240 S2=S2+SIGN*T/I
250 T=T*A2SQ
260 I=I+2
270 SIGN=-SIGN
280 WEND
290 PI=16*S1-4*S2
300 T1=TIMER
310 PRINT "PI=";INT(PI*N+0.5)/N
320 PRINT "TIME=";INT(T1-T0)
330 END
